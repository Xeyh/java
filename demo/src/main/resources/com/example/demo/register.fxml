<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/9.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" prefHeight="223.0" prefWidth="338.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"  xmlns:fx="http://javafx.com/fxml/1"   >
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
        <Label text="Username" />
            <TextField fx:id="username"/>

        <Label prefHeight="25.0" prefWidth="58.0" text="Password" />
            <TextField fx:id="password"/>
            <Button mnemonicParsing="false" text="Submit" onAction="#OnclickSubmit" />
    </VBox>
</fx:root>